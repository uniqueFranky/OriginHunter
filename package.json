{
  "name": "originhunter",
  "displayName": "OriginHunter",
  "description": "Discovering origins of code changes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "OriginHunter.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "OriginHunter.mineHistoryByMethodSignature",
        "title": "Mine History By Method Signature"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "originhunter.nameLLM": {
          "type": "string",
          "default": "qwen2.5-coder:7b",
          "description": "The name of the LLM to use"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npx webpack --config webpack.config.js",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.2.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "extensionDependencies": [
    "vscode.git"
  ],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.3.0",
    "ollama": "^0.5.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-syntax-highlighter": "^15.6.1",
    "tree-sitter": "^0.22.1",
    "tree-sitter-java": "^0.23.4",
    "tree-sitter-javascript": "^0.23.1",
    "tree-sitter-python": "^0.23.5"
  }
}
